{
  "framework": "vite",
  "buildCommand": "npm run vercel-build",
  "devCommand": "npm run dev",
  "installCommand": "pnpm install",
  "outputDirectory": "dist",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10
    }
  }
}
