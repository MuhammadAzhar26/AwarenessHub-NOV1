{
  "buildCommand": "cd $VERCEL_PROJECT_ROOT && pnpm run build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install --shamefully-hoist",
  "devCommand": "pnpm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10
    }
  }
}
